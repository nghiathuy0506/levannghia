#include <iostream>
using namespace std;
int main(){
	int a;
	cout << "nhap a:  ";
	cin >> a;
	int b;
	cout << "nhap b:  ";
	cin >> b;
	int tong = a + b;
	tong = a + b;
	int hieu = a - b;
	hieu = a - b;
	int tich = a * b;
	tich = a * b;
	float thuong = (float) a / b;
	thuong = (float) a / b;
	cout << "tong = " << tong << endl;
	cout << "hieu = " << hieu << endl;
	cout << "tich = " << tich << endl;
	cout << "thuong = " << thuong << endl;
	}
